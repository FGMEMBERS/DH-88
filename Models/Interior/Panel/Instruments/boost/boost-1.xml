<?xml version="1.0" encoding="UTF-8"?>

<PropertyList> 

  <path>boost.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.028 </red>
      <green> 0.014 </green>
      <blue>  0.007 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation> 
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine[1]/boost-gauge-psi</property>
    <!--    <factor>0.4911</factor>
    <offset>-29.92</offset> -->
    <interpolation> 
      <entry><ind> -5 </ind><dep>  -85 </dep></entry>
      <entry><ind> -4 </ind><dep>  -86 </dep></entry>
      <entry><ind> -3 </ind><dep> -110 </dep></entry>
      <entry><ind> -2 </ind><dep> -130 </dep></entry>
      <entry><ind> -1 </ind><dep> -150 </dep></entry>
      <entry><ind>  0 </ind><dep>    0 </dep></entry>
      <entry><ind>  2 </ind><dep>   43 </dep></entry>
      <entry><ind>  4 </ind><dep>   85 </dep></entry>
      <entry><ind>  6 </ind><dep>  125 </dep></entry>
      <entry><ind>  8 </ind><dep>  165 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
