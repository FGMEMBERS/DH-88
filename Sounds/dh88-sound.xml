<?xml version="1.0" encoding="ISO-8859-1"?>
 
<PropertyList>

  <fx>
 
    <!-- Engine 1 -->
    <crank>
      <name>engstart</name>
      <path>Aircraft/DH-88/Sounds/startup.wav</path>
      <property>/engines/engine[0]/cranking</property>
      <volume>
        <property>/sim/model/sound/volume</property>
      </volume>
      <reference-dist>5</reference-dist>
      <max-dist>2</max-dist>
    </crank>

    <crank>
      <name>crank1</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <condition>
        <property>/engines/engine[0]/cranking</property>
        <not>
          <property>/engines/engine[0]/running</property>
        </not>
      </condition>
      <volume>
        <offset>0.5</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <crank>
      <name>cough1</name>
      <path>Sounds/coughing.wav</path>
      <property>/engines/engine[0]/running</property>
      <volume>
        <offset>0.3</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <engine>
      <name>engine-Midddle</name>
      <mode>looped</mode>
      <path>Aircraft/DH-88/Sounds/engine-middle.wav</path>
      <condition>
        <and>
          <property>/engines/engine[0]/running</property>
          <greater-than>
            <property>engines/engine[0]/rpm</property>
            <value>10</value>
          </greater-than>
          <less-than> 
            <property>/engines/engine[0]/rpm</property>
            <value>700</value>
          </less-than>
        </and>
      </condition>
      <pitch>
        <property>engines/engine[0]/rpm</property>
        <factor>0.0005</factor>
        <min>1.0</min>
        <max>2.0</max>
        <offset>0.10</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <engine>
      <name>engine-High</name>
      <mode>looped</mode>
      <path>Aircraft/DH-88/Sounds/engine-high.wav</path>
      <condition>
        <and>
          <property>/engines/engine[0]/running</property>
          <greater-than>
            <property>engines/engine[0]/rpm</property>
            <value>700</value>
          </greater-than>
          <less-than> 
            <property>/engines/engine[0]/rpm</property>
            <value>2500</value>
          </less-than>
        </and>
      </condition>
      <volume>
        <property>engines/engine[0]/rpm</property>
        <min>0.1</min>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>engines/engine[0]/rpm</property>
        <factor>0.0005</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0.1</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <!-- Engine 2 -->
    <crank>
      <name>engstart</name>
      <path>Aircraft/DH-88/Sounds/startup.wav</path>
      <property>/engines/engine[1]/cranking</property>
      <volume>
        <property>/sim/model/sound/volume</property>
      </volume>
      <reference-dist>5</reference-dist>
      <max-dist>2</max-dist>
    </crank>

    <crank>
      <name>crank1</name>
      <mode>looped</mode>
      <path>Sounds/cranking.wav</path>
      <condition>
        <property>/engines/engine[1]/cranking</property>
        <not>
          <property>/engines/engine[1]/running</property>
        </not>
      </condition>
      <volume>
        <offset>0.5</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <crank>
      <name>cough1</name>
      <path>Sounds/coughing.wav</path>
      <property>/engines/engine[1]/running</property>
      <volume>
        <offset>0.3</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <engine>
      <name>engine-Midddle</name>
      <mode>looped</mode>
      <path>Aircraft/DH-88/Sounds/engine-middle.wav</path>
      <condition>
        <and>
          <property>/engines/engine[1]/running</property>
          <greater-than>
            <property>engines/engine[1]/rpm</property>
            <value>10</value>
          </greater-than>
          <less-than> 
            <property>/engines/engine[1]/rpm</property>
            <value>700</value>
          </less-than>
        </and>
      </condition>
      <pitch>
        <property>engines/engine[1]/rpm</property>
        <factor>0.0005</factor>
        <min>1.0</min>
        <max>2.0</max>
        <offset>0.10</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <engine>
      <name>engine-High</name>
      <mode>looped</mode>
      <path>Aircraft/DH-88/Sounds/engine-high.wav</path>
      <condition>
        <and>
          <property>/engines/engine[1]/running</property>
          <greater-than>
            <property>engines/engine[1]/rpm</property>
            <value>700</value>
          </greater-than>
          <less-than> 
            <property>/engines/engine[1]/rpm</property>
            <value>2500</value>
          </less-than>
        </and>
      </condition>
      <volume>
        <property>engines/engine[1]/rpm</property>
        <min>0.1</min>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>engines/engine[1]/rpm</property>
        <factor>0.0005</factor>
        <min>0.3</min>
        <max>2.0</max>
        <offset>0.1</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

    <rumble>
      <name>rumble</name>
      <mode>looped</mode>
      <path>Sounds/rumble.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <type>log</type>
        <factor>0.5</factor>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </rumble>

    <gear>
      <name>gear</name>
      <mode>in-transit</mode>
      <path>Aircraft/DH-88/Sounds/gear.wav</path>
      <property>/gear/gear[0]/position-norm</property>
      <pitch>
        <internal>dt_play</internal>
        <offset>1.0</offset>
        <factor>-0.001</factor>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </gear>
 
    <gear>
      <name>gear-lock</name>
      <path>Sounds/gear-lck.wav</path>
      <condition>
        <greather-than>
          <property>/gear/gear[0]/position-norm</property>
          <value>0.01</value>
        </greather-than>
        <less-than>
          <property>/gear/gear[0]/position-norm</property>
          <value>0.1</value>
        </less-than>
      </condition>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </gear>

    <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.05</factor>
        <min>0.1</min>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>0.4</offset>
      </volume>
      <pitch>
        <offset>1.25</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal>
 
    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Aircraft/DH-88/Sounds/flaps.wav</path>
      <property>/surface-positions/flap-pos-norm</property>
      <volume>
        <offset>0.25</offset>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </flap>

    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Sounds/wind.wav</path>
      <property>/velocities/airspeed-kt</property>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.000015</factor>
        <offset>1.0</offset>
        <min>0.1</min>
        <max>1.0</max>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0015</factor>
        <min>0.03</min>
        <max>0.25</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
        <offset>1.25</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </wind>
 
    <stall>
      <name>stall</name>
      <path>Sounds/stall.wav</path>
      <property>/sim/aero/alarms/stall-warning</property>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <min>30.0</min>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </stall>
 
  </fx>
 
</PropertyList>
